{
    "name": "Handsome Wu's first extension",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
             "matches": ["<all_urls>"],
             "all_frames": true,
              "js": ["index.js"],
             "css": ["style.css"]
         }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*", 
        "https://*/*"
    ]
}
